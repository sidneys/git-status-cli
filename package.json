{
  "name": "git-status-cli",
  "appId": "de.sidneys.git-status-cli",
  "version": "0.5.0",
  "description": "Get the file status (`git status`) of the current Git repository from the commandline. Format the output as JSON or plaintext.",
  "license": "MIT",
  "homepage": "https://github.com/sidneys/git-status-cli",
  "author": {
    "name": "sidneys",
    "email": "sidneys.github.io@outlook.com",
    "url": "https://sidneys.github.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sidneys/git-status-cli.git"
  },
  "bugs": {
    "url": "https://github.com/sidneys/git-status-cli/issues"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "os": [
    "darwin",
    "win32",
    "linux"
  ],
  "keywords": [
    "git",
    "status",
    "commandline",
    "terminal",
    "repository",
    "terminal",
    "formatted",
    "file status",
    "untracked",
    "tracked",
    "modified",
    "unmodified",
    "text",
    "commit",
    "message",
    "json"
  ],
  "preferGlobal": true,
  "dependencies": {
    "@sidneys/logger": "^1.3.0",
    "app-root-path": "^2.0.1",
    "chalk": "^2.4.1",
    "git-status-utils": "^0.3.3",
    "indent-string": "^3.2.0",
    "json-stringify-pretty-compact": "^1.2.0",
    "minimist": "^1.2.0",
    "pad": "^2.1.0",
    "parse-git-status": "^0.1.0",
    "window-size": "^1.1.0",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "jsdoc": "^3.5.5"
  },
  "main": "./lib/main.js",
  "bin": {
    "git-status": "./lib/main.js"
  },
  "scripts": {
    "docs": "./node_modules/.bin/jsdoc --verbose --configure .jsdoc.json",
    "start": "node ./lib/main.js"
  }
}
